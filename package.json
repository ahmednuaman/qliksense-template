{
  "name": "qliksense-template",
  "version": "1.0.0",
  "description": "A simple boilerplate for writing Qlik Sense templates in Javascript",
  "scripts": {
    "browser-sync": "node ./browser-sync.js",
    "build": "concurrently \"npm test\" \"npm run webpack\"",
    "deploy": "cross-env NODE_ENV=\"production\" npm run build",
    "local": "concurrently \"npm run browser-sync\" \"npm run watch\"",
    "sass-lint": "sass-lint 'src/scss/*.scss' -v -q",
    "standard": "standard",
    "start": "npm run local",
    "test": "concurrently \"npm run standard\" \"npm run sass-lint\"",
    "watch": "concurrently \"npm run webpack -- --watch -d\" \"npm run watch:js\" \"npm run watch:scss\"",
    "watch:js": "onchange \"**/*.js\" \"qlik/index.html\" -- npm run standard",
    "watch:scss": "onchange \"src/scss/*.scss\" -- npm run sass-lint",
    "webpack": "webpack --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmednuaman/0212VF-qliksense-template.git"
  },
  "keywords": [
    "qliksense"
  ],
  "author": "Ahmed Nuaman (ahmed@ahmednuaman.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahmednuaman/0212VF-qliksense-template/issues"
  },
  "homepage": "https://github.com/ahmednuaman/0212VF-qliksense-template#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-sync": "^2.18.5",
    "clean-webpack-plugin": "^0.1.14",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.2.0",
    "onchange": "^3.2.1",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.1.1",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-stats-plugin": "^0.1.4",
    "zip-webpack-plugin": "^0.3.0"
  },
  "babel": {
    "plugins": [
      "lodash"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "build",
      "qlik/*.js"
    ]
  },
  "dependencies": {
    "angular": "^1.6.1",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "bootswatch": "^3.3.7",
    "font-awesome": "^4.7.0",
    "global": "^4.3.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "source-sans-pro": "^2.0.10"
  }
}
